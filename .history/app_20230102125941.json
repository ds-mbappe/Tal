{
  "expo": {
    "name": "Tal",
    "slug": "Tal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/accueil.png",
      "resizeMode": "contain"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "TalCS a besoin d'accéder à votre galerie photo"
        }
      ]
    ],
    "packagerOpts": {
      "sourceExts": ["cjs"]
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
